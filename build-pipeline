            steps {
                echo "Request Build ID"
            }
        }
        
        stage('Create Build zip') {
            steps {
                echo "Create Build zip"
            }
        }
        
        stage('Copy/Replace Build zip') {
            steps {
                echo "Copy/Replace Build zip"
            }
        }
        
        stage('Update Build location') {
            steps {
                echo "Update Build location"
            }
        }
    }
}
